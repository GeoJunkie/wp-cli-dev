	"require": {
		"php": ">=5.6",
		"ext-json": "*",
		"wp-cli/admin-command": "dev-master",
		"wp-cli/automated-tests": "dev-master",
		"wp-cli/cache-command": "dev-master",
		"wp-cli/checksum-command": "dev-master",
		"wp-cli/config-command": "dev-master",
		"wp-cli/core-command": "dev-master",
		"wp-cli/cron-command": "dev-master",
		"wp-cli/db-command": "dev-master",
		"wp-cli/dist-archive-command": "dev-master",
		"wp-cli/doctor-command": "dev-master",
		"wp-cli/embed-command": "dev-master",
		"wp-cli/entity-command": "dev-master",
		"wp-cli/eval-command": "dev-master",
		"wp-cli/export-command": "dev-master",
		"wp-cli/extension-command": "dev-master",
		"wp-cli/find-command": "dev-master",
		"wp-cli/google-sitemap-generator-cli": "dev-master",
		"wp-cli/i18n-command": "dev-master",
		"wp-cli/import-command": "dev-master",
		"wp-cli/language-command": "dev-master",
		"wp-cli/maintenance-mode-command": "dev-master",
		"wp-cli/media-command": "dev-master",
		"wp-cli/mustangostang-spyc": "dev-master as 0.6.x-dev",
		"wp-cli/package-command": "dev-master",
		"wp-cli/php-cli-tools": "dev-master as 0.11.x-dev",
		"wp-cli/profile-command": "dev-master",
		"wp-cli/regenerate-readme": "dev-master",
		"wp-cli/restful": "dev-master",
		"wp-cli/rewrite-command": "dev-master",
		"wp-cli/role-command": "dev-master",
		"wp-cli/scaffold-command": "dev-master",
		"wp-cli/scaffold-package-command": "dev-master",
		"wp-cli/search-replace-command": "dev-master",
		"wp-cli/server-command": "dev-master",
		"wp-cli/shell-command": "dev-master",
		"wp-cli/super-admin-command": "dev-master",
		"wp-cli/widget-command": "dev-master",
		"wp-cli/wp-cli": "dev-master",
		"wp-cli/wp-cli-bundle": "dev-master",
		"wp-cli/wp-cli-tests": "dev-master",
		"wp-cli/wp-config-transformer": "dev-master as 1.2.x-dev",
		"wp-cli/wp-super-cache-cli": "dev-master"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master"
	},
	"suggest": {
		"psy/psysh": "Enhanced `wp shell` functionality"
	},
	"config": {
		"platform": {
			"php": "5.6"
		},
		"process-timeout": 7200,
		"sort-packages": true
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.0.x-dev"
		},
        "commands": [
            "maintenance",
			"maintenance contrib-list",
			"maintenance milestones-after",
			"maintenance milestones-since",
			"maintenance release-date",
			"maintenance release-notes",
			"maintenance replace-label"
        ],
		"readme": {
            "sections": [
	            "Installation",
	            "Development",
                "Using",
                "Contributing",
                "Support"
            ],
            "package_description": {
                "post": ".readme-partials/DESCRIPTION.md"
            },
			"development": {
                "body": ".readme-partials/DEVELOPMENT.md"
            },
			"installation": {
                "body": ".readme-partials/INSTALLATION.md"
            },
            "show_powered_by": false
        }
    },
	"autoload-dev": {
		"psr-4": {
			"WP_CLI\\Maintenance\\": ".maintenance/src/"
		},
		"files": [
			".maintenance/bootstrap.php"
		]
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"pre-install-cmd": "php .maintenance/clone-all-repositories.php",
		"pre-update-cmd": "php .maintenance/clone-all-repositories.php",
		"post-install-cmd": "php .maintenance/symlink-vendor-folders.php",
		"post-update-cmd": "php .maintenance/symlink-vendor-folders.php",
		"behat": "run-behat-tests",
		"behat-rerun": "rerun-behat-tests",
		"lint": "run-linter-tests",
		"phpcs": "run-phpcs-tests",
		"phpunit": "run-php-unit-tests",
		"prepare-tests": "install-package-tests",
		"test": [
			"@lint",
			"@phpcs",
			"@phpunit",
			"@behat"
		]
	},
	"support": {
		"issues": "https://github.com/wp-cli/wp-cli-bundle/issues",
		"source": "https://github.com/wp-cli/wp-cli-bundle",
		"docs": "https://make.wordpress.org/cli/handbook/"
	}
}
